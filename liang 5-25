public class Main {

    public static void main(String[] args) {
              for(int i = 10000; i<=100000; i += 10000)
                {
                  System.out.println("i = "+i);
                  double pi=0;
                  for(int j=1;j<=i;j++)
                    {
                         pi =pi+(Math.pow(-1,(j+1)))/(2*j-1);
                    }
                  pi=4*pi;
                  System.out.println("pi = "+pi);
              }
         }
   }
